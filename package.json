{
  "name": "pixijs-map-layer",
  "version": "1.1.0",
  "description": "Google maps extension that uses Pixi.JS to efficiently render GeoJSON data.",
  "main": "lib/index.js",
  "scripts": {
    "start": "watchify src/app/app.js -o public/app.js -t [ babelify --presets [es2015 stage-0 ] ]",
    "start:lib": "babel --watch src/lib -d lib",
    "build:app": "browserify src/app/app.js -o public/app.js -t [ babelify --presets [es2015 stage-0 ] ]",
    "build:lib": "babel src/lib -d lib",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RishabhJain96/pixijs-map-layer.git"
  },
  "keywords": [
    "PixiJS",
    "google",
    "maps",
    "google-maps",
    "WebGL"
  ],
  "author": {
    "name": "Rishabh Jain",
    "email": "rj254@cornell.edu"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RishabhJain96/pixijs-map-layer/issues"
  },
  "homepage": "https://github.com/RishabhJain96/pixijs-map-layer#readme",
  "dependencies": {
    "canvaslayer": "github:rishabhjain96/CanvasLayer",
    "pixi.js": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "standard-version": "^3.0.0",
    "watchify": "^3.7.0"
  }
}
